{"ast":null,"code":"import BaseCard from '@/components/BaseCard';\nimport { mapState, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCart',\n  components: {\n    BaseCard\n  },\n\n  data() {\n    return {\n      form: {\n        first_name: '',\n        last_name: '',\n        phone: '',\n        discount_code: '',\n        labels: ''\n      },\n      labels: {\n        delete: 'Delete',\n        cart: 'Cart',\n        quantity: 'Quantity',\n        back_to_catalog: 'Back to catalog',\n        go_to_catalog: 'Go to catalog',\n        empty_cart: 'Empty cart',\n        first_name: 'First Name',\n        last_name: 'Last Name',\n        phone: 'Phone',\n        country: 'Country',\n        discount_code: 'Discount code'\n      }\n    };\n  },\n\n  props: {},\n  computed: { ...mapState({\n      cart: 'cart'\n    })\n  },\n  methods: { ...mapActions({\n      deleteProductFromCart: 'deleteProductFromCart',\n      incrementQuantity: 'incrementQuantity',\n      decrementQuantity: 'decrementQuantity'\n    }),\n\n    deleteProduct(index) {\n      this.deleteProductFromCart(index);\n    },\n\n    increment(index) {\n      this.incrementQuantity(index);\n    },\n\n    decrement(index) {\n      this.decrementQuantity(index);\n    },\n\n    sth() {\n      console.log('sth');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0EA,OAAOA,QAAP,MAAqB,uBAArB;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAqC,MAArC;AAEA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE;IACRJ;EADQ,CAFD;;EAKXK,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFC,UAAU,EAAE,EADV;QAEFC,SAAS,EAAE,EAFT;QAGFC,KAAK,EAAE,EAHL;QAIFC,aAAa,EAAE,EAJb;QAKFC,MAAM,EAAE;MALN,CADH;MAQHA,MAAM,EAAE;QACJC,MAAM,EAAE,QADJ;QAEJC,IAAI,EAAE,MAFF;QAGJC,QAAQ,EAAE,UAHN;QAIJC,eAAe,EAAE,iBAJb;QAKJC,aAAa,EAAE,eALX;QAMJC,UAAU,EAAE,YANR;QAOJV,UAAU,EAAE,YAPR;QAQJC,SAAS,EAAE,WARP;QASJC,KAAK,EAAE,OATH;QAUJS,OAAO,EAAE,SAVL;QAWJR,aAAa,EAAE;MAXX;IARL,CAAP;EAsBH,CA5BU;;EA6BXS,KAAK,EAAE,EA7BI;EA8BXC,QAAQ,EAAE,EACN,GAAGnB,QAAQ,CAAC;MACRY,IAAI,EAAE;IADE,CAAD;EADL,CA9BC;EAmCXQ,OAAO,EAAE,EACL,GAAGnB,UAAU,CAAC;MACXoB,qBAAqB,EAAE,uBADZ;MAEXC,iBAAiB,EAAE,mBAFR;MAGXC,iBAAiB,EAAE;IAHR,CAAD,CADR;;IAMLC,aAAa,CAACC,KAAD,EAAQ;MACjB,KAAKJ,qBAAL,CAA2BI,KAA3B;IACH,CARI;;IASLC,SAAS,CAACD,KAAD,EAAQ;MACb,KAAKH,iBAAL,CAAuBG,KAAvB;IACH,CAXI;;IAYLE,SAAS,CAACF,KAAD,EAAQ;MACb,KAAKF,iBAAL,CAAuBE,KAAvB;IACH,CAdI;;IAeLG,GAAG,GAAG;MACFC,OAAO,CAACC,GAAR,CAAY,KAAZ;IACJ;;EAjBK;AAnCE,CAAf","names":["BaseCard","mapState","mapActions","name","components","data","form","first_name","last_name","phone","discount_code","labels","delete","cart","quantity","back_to_catalog","go_to_catalog","empty_cart","country","props","computed","methods","deleteProductFromCart","incrementQuantity","decrementQuantity","deleteProduct","index","increment","decrement","sth","console","log"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CV/VUE-JS/online_shop/src/components/ShopCart.vue"],"sourcesContent":["<template>\n    <div>\n        <h1>{{ labels.cart }}</h1>\n        <div class=\"cart\">\n            {{ form }}\n            <form class=\"form\" :action=\"sth\">\n                <label for=\"fname\">{{ labels.first_name }}</label>\n                <input\n                    type=\"text\"\n                    id=\"fname\"\n                    name=\"firstname\"\n                    placeholder=\"Your name..\"\n                    v-model=\"form.first_name\"\n                >\n\n                <label for=\"lname\">{{ labels.last_name }}</label>\n                <input type=\"text\" id=\"lname\" name=\"lastname\" placeholder=\"Your last name..\">\n\n                <label for=\"fname\">{{ labels.phone }}</label>\n                <input type=\"tel\" id=\"phone\" name=\"phone\" placeholder=\"Phone..\">\n\n                <label for=\"fname\">{{ labels.discount_code }}</label>\n                <input type=\"tel\" id=\"discount_code\" name=\"discount_code\" placeholder=\"Discount code..\">\n\n                <label for=\"country\">{{ labels.country }}</label>\n                <select id=\"country\" name=\"country\">\n                <option value=\"australia\">Australia</option>\n                <option value=\"canada\">Canada</option>\n                <option value=\"usa\">USA</option>\n                </select>\n            \n                <input type=\"submit\" value=\"Submit\">\n            </form>\n            <div class=\"products\">\n                <div class=\"product\" v-for=\"(item, index) in cart\" :key=\"index\">\n                    <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n                    <p>{{ item.name }}</p>\n                    <p>{{ item.price }}â‚´</p>\n                    <div style=\"display: flex\">\n                        <button\n                            @click=\"decrement(index)\"\n                        >\n                            -\n                        </button>\n                        <p>{{ labels.quantity }}: {{ item.quantity }}</p>\n                        <button\n                            @click=\"increment(index)\"\n                        >\n                            +\n                        </button>\n                    </div>\n                    <button\n                        class=\"button\"\n                        type=\"button\"\n                        @click=\"deleteProduct(index)\"\n                    >\n                        {{ labels.delete }}\n                    </button>\n                </div>\n                <div v-if=\"!this.cart.length\">\n                    {{ labels.empty_cart }}!\n                    <router-link :to=\"{name: 'catalog'}\">\n                        {{ labels.go_to_catalog }}\n                    </router-link>\n                </div>\n            </div>\n        </div>\n        <BaseCard background-color=\"red\">\n            Hello!\n        </BaseCard>\n    </div>\n</template>\n\n<script>\nimport BaseCard from '@/components/BaseCard';\nimport { mapState, mapActions } from 'vuex';\n\nexport default {\n    name: 'ShopCart',\n    components: {\n        BaseCard,\n    },\n    data() {\n        return {\n            form: {\n                first_name: '',\n                last_name: '',\n                phone: '',\n                discount_code: '',\n                labels: ''\n            },\n            labels: {\n                delete: 'Delete',\n                cart: 'Cart',\n                quantity: 'Quantity',\n                back_to_catalog: 'Back to catalog',\n                go_to_catalog: 'Go to catalog',\n                empty_cart: 'Empty cart',\n                first_name: 'First Name',\n                last_name: 'Last Name',\n                phone: 'Phone',\n                country: 'Country',\n                discount_code: 'Discount code',\n            }\n        }\n    },\n    props: {},\n    computed: {\n        ...mapState({\n            cart: 'cart'\n        })\n    },\n    methods: {\n        ...mapActions({\n           deleteProductFromCart: 'deleteProductFromCart',\n           incrementQuantity: 'incrementQuantity',\n           decrementQuantity: 'decrementQuantity',\n        }),\n        deleteProduct(index) {\n            this.deleteProductFromCart(index);\n        },\n        increment(index) {\n            this.incrementQuantity(index);\n        },\n        decrement(index) {\n            this.decrementQuantity(index);\n        },\n        sth() {\n            console.log('sth')\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.cart {\n    display: flex;\n    padding-top: 80px;\n    .form {\n        input[type=text], input[type=tel], select {\n            width: 100%;\n            padding: 12px 20px;\n            margin: 8px 0;\n            display: inline-block;\n            border: 1px solid #ccc;\n            border-radius: 4px;\n            box-sizing: border-box;\n        }\n\n        input[type=submit] {\n            width: 100%;\n            background-color: #4CAF50;\n            color: white;\n            padding: 14px 20px;\n            margin: 8px 0;\n            border: none;\n            border-radius: 4px;\n            cursor: pointer;\n        }\n\n        input[type=submit]:hover {\n            background-color: #45a049;\n        }\n\n        div {\n            border-radius: 5px;\n            background-color: #f2f2f2;\n            padding: 20px;\n        }\n    }\n\n    .products {\n        overflow-y: scroll;\n        width: 60%;\n        height: 678px;\n        .product {\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n            background-color: #e9e9e9;\n            box-shadow: 8px 8px 22px #e9d7d7, -8px -8px 22px #cddbcd;\n            margin: 8px;\n            padding: 8px;\n\n            .button {\n                padding: 10px;\n                border-radius: 50px;\n                background: #2196F3;\n                color: white;\n                font-weight: bold;\n                cursor: pointer;\n                border: none;\n                transition: .3s ease;\n\n                &:hover {\n                    opacity: .8;\n                }\n            }\n\n            img {\n                height: 200px;\n            }\n        }\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}