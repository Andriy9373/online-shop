{"ast":null,"code":"import { mapState, mapGetters, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCatalog',\n  components: {// ShopCatalogItem\n  },\n  props: {},\n\n  data() {\n    return {\n      labels: {\n        add_to_cart: 'Add to cart'\n      }\n    };\n  },\n\n  computed: { ...mapState({\n      products: 'products',\n      cart: 'cart'\n    }),\n    ...mapGetters({\n      getTotalPrice: 'getTotalPrice'\n    })\n  },\n  methods: { ...mapActions({\n      getProducts: 'getProducts'\n    })\n  },\n\n  mounted() {\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,SAASA,QAAT,EAAmBC,UAAnB,EAA+BC,UAA/B,QAAiD,MAAjD;AAEA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE,CACR;EADQ,CAFD;EAKXC,KAAK,EAAE,EALI;;EAMXC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE;QACJC,WAAW,EAAE;MADT;IADL,CAAP;EAKH,CAZU;;EAaXC,QAAQ,EAAE,EACN,GAAGT,QAAQ,CAAC;MACRU,QAAQ,EAAE,UADF;MAERC,IAAI,EAAE;IAFE,CAAD,CADL;IAKN,GAAGV,UAAU,CAAC;MACVW,aAAa,EAAE;IADL,CAAD;EALP,CAbC;EAsBXC,OAAO,EAAE,EACL,GAAGX,UAAU,CAAC;MACVY,WAAW,EAAE;IADH,CAAD;EADR,CAtBE;;EA2BXC,OAAO,GAAG;IACN,KAAKD,WAAL;EACJ;;AA7BW,CAAf","names":["mapState","mapGetters","mapActions","name","components","props","data","labels","add_to_cart","computed","products","cart","getTotalPrice","methods","getProducts","mounted"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CD/VUE-JS/online_shop/src/components/ShopCatalog.vue"],"sourcesContent":["<template>\n    <h1>Catalog!</h1>\n    <div class=\"catalog\">\n        <div class=\"products\">\n            <router-link :to=\"{name: 'Cart'}\" class=\"link_to_cart\">\n                Cart: {{ this.cart.length }}\n            </router-link>\n            <div\n                class=\"catalog-item\"\n                v-for=\"(item, index) in products\"\n                :key=index\n            >\n                <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n                <h2>{{ item.name }}</h2>\n                <p>{{ item.article }}</p>\n                <p>{{ item.price }}</p>\n                <button @click=\"$emit('addToCart', item)\">\n                    {{ labels.add_to_cart }}\n                </button>\n            </div>\n        </div>\n        <div class=\"total\">\n            <p>Total: {{ this.getTotalPrice }} UAH</p>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapState, mapGetters, mapActions } from 'vuex';\n\nexport default {\n    name: 'ShopCatalog',\n    components: {\n        // ShopCatalogItem\n    },\n    props: {},\n    data() {\n        return {\n            labels: {\n                add_to_cart: 'Add to cart'\n            }\n        }\n    },\n    computed: {\n        ...mapState({\n            products: 'products',\n            cart: 'cart'\n        }),\n        ...mapGetters({\n            getTotalPrice: 'getTotalPrice'\n        })\n    },\n    methods: {\n        ...mapActions({\n            getProducts: 'getProducts',\n        })\n    },\n    mounted() {\n        this.getProducts();\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.catalog {\n    .products {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        flex-wrap: wrap;\n\n        .link_to_cart {\n            position: absolute;\n            top: 10px;\n            right: 150px;\n            padding: 16px;\n            border: 1px solid black;\n        }\n\n        .catalog-item {\n            // background-color: $background-color;\n            background-color: orange;\n            flex-basis: 25%;\n            // box-shadow: $box-shadow;\n            margin: 8px;\n            padding: 8px;\n            img {\n                height: 200px;\n            }\n        }\n    }\n\n    .total {\n        position: fixed;\n        background-color: green;\n        color: #ffffff;\n        padding: 10px;\n        top: 0;\n        right: 0;\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}