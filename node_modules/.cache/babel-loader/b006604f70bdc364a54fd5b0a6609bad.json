{"ast":null,"code":"import CustomSelect from '@/components/CustomSelect';\nimport { mapState, mapGetters, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCatalog',\n  components: {\n    CustomSelect\n  },\n  props: {},\n\n  data() {\n    return {\n      options: [{\n        name: 'Male',\n        value: 'Male'\n      }, {\n        name: 'Female',\n        value: 'Female'\n      }, {\n        name: 'All',\n        value: 'All'\n      }],\n      selected: 'All',\n      labels: {\n        add_to_cart: 'Add to cart'\n      }\n    };\n  },\n\n  computed: { ...mapState({\n      products: 'products',\n      cart: 'cart'\n    }),\n    ...mapGetters({\n      getTotalPrice: 'getTotalPrice'\n    })\n  },\n  methods: { ...mapActions({\n      getProducts: 'getProducts'\n    }),\n\n    selected(value) {\n      console.log(value);\n    }\n\n  },\n\n  mounted() {\n    // console.log(this.$route.name)\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AA0BA,OAAOA,YAAP,MAAyB,2BAAzB;AACA,SAASC,QAAT,EAAmBC,UAAnB,EAA+BC,UAA/B,QAAiD,MAAjD;AAEA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE;IACRL;EADQ,CAFD;EAKXM,KAAK,EAAE,EALI;;EAMXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,CACL;QAACJ,IAAI,EAAE,MAAP;QAAeK,KAAK,EAAE;MAAtB,CADK,EAEL;QAACL,IAAI,EAAE,QAAP;QAAiBK,KAAK,EAAE;MAAxB,CAFK,EAGL;QAACL,IAAI,EAAE,KAAP;QAAcK,KAAK,EAAE;MAArB,CAHK,CADN;MAMHC,QAAQ,EAAE,KANP;MAOHC,MAAM,EAAE;QACJC,WAAW,EAAE;MADT;IAPL,CAAP;EAWH,CAlBU;;EAmBXC,QAAQ,EAAE,EACN,GAAGZ,QAAQ,CAAC;MACRa,QAAQ,EAAE,UADF;MAERC,IAAI,EAAE;IAFE,CAAD,CADL;IAKN,GAAGb,UAAU,CAAC;MACVc,aAAa,EAAE;IADL,CAAD;EALP,CAnBC;EA4BXC,OAAO,EAAE,EACL,GAAGd,UAAU,CAAC;MACVe,WAAW,EAAE;IADH,CAAD,CADR;;IAILR,QAAQ,CAACD,KAAD,EAAQ;MACZU,OAAO,CAACC,GAAR,CAAYX,KAAZ;IACJ;;EANK,CA5BE;;EAoCXY,OAAO,GAAG;IACN;IACA,KAAKH,WAAL;EACJ;;AAvCW,CAAf","names":["CustomSelect","mapState","mapGetters","mapActions","name","components","props","data","options","value","selected","labels","add_to_cart","computed","products","cart","getTotalPrice","methods","getProducts","console","log","mounted"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CD/VUE-JS/online_shop/src/components/ShopCatalog.vue"],"sourcesContent":["<template>\n    <h1>Catalog!</h1>\n    <CustomSelect :options=\"options\" :selected=\"selected\" @selected=\"selected\"/>\n    <div class=\"catalog\">\n        <div class=\"products\">\n            <div\n                class=\"catalog-item\"\n                v-for=\"(item, index) in products\"\n                :key=index\n            >\n                <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n                <h2>{{ item.name }}</h2>\n                <p>{{ item.article }}</p>\n                <p>{{ item.price }}</p>\n                <button @click=\"$emit('addToCart', item)\">\n                    {{ labels.add_to_cart }}\n                </button>\n            </div>\n        </div>\n        <div class=\"total\">\n            <p>Total: {{ this.getTotalPrice }} UAH</p>\n        </div>\n    </div>\n</template>\n\n<script>\nimport CustomSelect from '@/components/CustomSelect';\nimport { mapState, mapGetters, mapActions } from 'vuex';\n\nexport default {\n    name: 'ShopCatalog',\n    components: {\n        CustomSelect\n    },\n    props: {},\n    data() {\n        return {\n            options: [\n                {name: 'Male', value: 'Male'},\n                {name: 'Female', value: 'Female'},\n                {name: 'All', value: 'All'},\n            ],\n            selected: 'All',\n            labels: {\n                add_to_cart: 'Add to cart'\n            }\n        }\n    },\n    computed: {\n        ...mapState({\n            products: 'products',\n            cart: 'cart',\n        }),\n        ...mapGetters({\n            getTotalPrice: 'getTotalPrice'\n        })\n    },\n    methods: {\n        ...mapActions({\n            getProducts: 'getProducts',\n        }),\n        selected(value) {\n            console.log(value)\n        }\n    },\n    mounted() {\n        // console.log(this.$route.name)\n        this.getProducts();\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.catalog {\n    .products {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        flex-wrap: wrap;\n\n        .catalog-item {\n            // background-color: $background-color;\n            background-color: orange;\n            flex-basis: 25%;\n            // box-shadow: $box-shadow;\n            margin: 8px;\n            padding: 8px;\n            img {\n                height: 200px;\n            }\n        }\n    }\n\n    .total {\n        position: fixed;\n        background-color: green;\n        color: #ffffff;\n        padding: 10px;\n        top: 0;\n        right: 0;\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}