{"ast":null,"code":"import CustomSelect from '@/components/Custom/CustomSelect';\nimport CustomRangeSlider from '@/components/Custom/CustomRangeSlider';\nimport { mapState, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCatalog',\n  components: {\n    CustomSelect,\n    CustomRangeSlider\n  },\n  props: {},\n\n  data() {\n    return {\n      options: [{\n        gender: 'Male'\n      }, {\n        gender: 'Female'\n      }, {\n        gender: 'All'\n      }],\n      selected: 'All',\n      labels: {\n        add_to_cart: 'Add to cart'\n      }\n    };\n  },\n\n  computed: { ...mapState({\n      products: 'products',\n      cart: 'cart'\n    }),\n\n    sortedProducts() {\n      console.log(this.selected);\n\n      if (this.selected === 'Male') {\n        const male = [];\n        this.products.forEach(product => {\n          if (product.category === 'Male') male.push(product);\n        });\n        return male;\n      } else if (this.selected === 'Female') {\n        const female = [];\n        this.products.forEach(product => {\n          if (product.category === 'Female') female.push(product);\n        });\n        return female;\n      }\n\n      return this.products;\n    }\n\n  },\n  methods: { ...mapActions({\n      getProducts: 'getProducts'\n    }),\n\n    selectedCategory(selected) {\n      this.selected = selected;\n    }\n\n  },\n\n  mounted() {\n    // console.log(this.$route.name)\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAqDA,OAAOA,YAAP,MAAyB,kCAAzB;AACA,OAAOC,iBAAP,MAA8B,uCAA9B;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAqC,MAArC;AAEA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE;IACRL,YADQ;IAERC;EAFQ,CAFD;EAMXK,KAAK,EAAE,EANI;;EAOXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,CACL;QAACC,MAAM,EAAE;MAAT,CADK,EAEL;QAACA,MAAM,EAAE;MAAT,CAFK,EAGL;QAACA,MAAM,EAAE;MAAT,CAHK,CADN;MAMHC,QAAQ,EAAE,KANP;MAOHC,MAAM,EAAE;QACJC,WAAW,EAAE;MADT;IAPL,CAAP;EAWH,CAnBU;;EAoBXC,QAAQ,EAAE,EACN,GAAGX,QAAQ,CAAC;MACRY,QAAQ,EAAE,UADF;MAERC,IAAI,EAAE;IAFE,CAAD,CADL;;IAKNC,cAAc,GAAG;MACbC,OAAO,CAACC,GAAR,CAAY,KAAKR,QAAjB;;MACA,IAAI,KAAKA,QAAL,KAAkB,MAAtB,EAA8B;QAC1B,MAAMS,IAAG,GAAI,EAAb;QACA,KAAKL,QAAL,CAAcM,OAAd,CAAsBC,OAAM,IAAK;UAC7B,IAAIA,OAAO,CAACC,QAAR,KAAqB,MAAzB,EAAiCH,IAAI,CAACI,IAAL,CAAUF,OAAV;QACpC,CAFD;QAGA,OAAOF,IAAP;MACJ,CANA,MAOK,IAAI,KAAKT,QAAL,KAAkB,QAAtB,EAAgC;QACjC,MAAMc,MAAK,GAAI,EAAf;QACA,KAAKV,QAAL,CAAcM,OAAd,CAAsBC,OAAM,IAAK;UAC7B,IAAIA,OAAO,CAACC,QAAR,KAAqB,QAAzB,EAAmCE,MAAM,CAACD,IAAP,CAAYF,OAAZ;QACtC,CAFD;QAGA,OAAOG,MAAP;MACJ;;MACA,OAAO,KAAKV,QAAZ;IACJ;;EAtBM,CApBC;EA4CXW,OAAO,EAAE,EACL,GAAGtB,UAAU,CAAC;MACVuB,WAAW,EAAE;IADH,CAAD,CADR;;IAILC,gBAAgB,CAACjB,QAAD,EAAW;MACvB,KAAKA,QAAL,GAAgBA,QAAhB;IACH;;EANI,CA5CE;;EAoDXkB,OAAO,GAAG;IACN;IACA,KAAKF,WAAL;EACJ;;AAvDW,CAAf","names":["CustomSelect","CustomRangeSlider","mapState","mapActions","name","components","props","data","options","gender","selected","labels","add_to_cart","computed","products","cart","sortedProducts","console","log","male","forEach","product","category","push","female","methods","getProducts","selectedCategory","mounted"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CV/VUE-JS/online_shop/src/components/ShopCatalog.vue"],"sourcesContent":["<template>\n    <h1>Catalog!</h1>\n    <CustomSelect\n        :options=\"options\"\n        :selected=\"selected\"\n        @selected-category=\"selectedCategory\"\n    />\n    <CustomRangeSlider/>\n    <div class=\"catalog\">\n        <div class=\"products\">\n            <div\n                class=\"catalog-item\"\n                v-for=\"(item, index) in sortedProducts\"\n                :key=index\n            >\n                <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n                <h2>{{ item.name }}</h2>\n                <p>{{ item.article }}</p>\n                <p>{{ item.price }}</p>\n                <button @click=\"$emit('addToCart', item)\">\n                    {{ labels.add_to_cart }}\n                </button>\n            </div>\n        </div>\n    </div>\n    <div class=\"container\">\n    <div class=\"card__container\">\n        <div class=\"card\">\n        <div class=\"card__content\">\n            <h3 class=\"card__header\">Card 1</h3>\n            <p class=\"card__info\">Lorem ipsum dolor sit amet consectetur adipisicing elit.</p>\n            <button class=\"card__button\">Read now</button>\n        </div>\n        </div>\n        <div class=\"card\">\n        <div class=\"card__content\">\n            <h3 class=\"card__header\">Card 2</h3>\n            <p class=\"card__info\">Lorem ipsum dolor sit amet consectetur adipisicing elit.</p>\n            <button class=\"card__button\">Read now</button>\n        </div>\n        </div>\n        <div class=\"card\">\n        <div class=\"card__content\">\n            <h3 class=\"card__header\">Card 3</h3>\n            <p class=\"card__info\">Lorem ipsum dolor sit amet consectetur adipisicing elit.</p>\n            <button class=\"card__button\">Read now</button>\n        </div>\n        </div>\n    </div>\n    </div>\n</template>\n\n<script>\nimport CustomSelect from '@/components/Custom/CustomSelect';\nimport CustomRangeSlider from '@/components/Custom/CustomRangeSlider';\nimport { mapState, mapActions } from 'vuex';\n\nexport default {\n    name: 'ShopCatalog',\n    components: {\n        CustomSelect,\n        CustomRangeSlider,\n    },\n    props: {},\n    data() {\n        return {\n            options: [\n                {gender: 'Male'},\n                {gender: 'Female'},\n                {gender: 'All'},\n            ],\n            selected: 'All',\n            labels: {\n                add_to_cart: 'Add to cart'\n            }\n        }\n    },\n    computed: {\n        ...mapState({\n            products: 'products',\n            cart: 'cart',\n        }),\n        sortedProducts() {\n            console.log(this.selected)\n            if (this.selected === 'Male') {\n                const male = [];\n                this.products.forEach(product => {\n                    if (product.category === 'Male') male.push(product)\n                })\n                return male;\n            }\n            else if (this.selected === 'Female') {\n                const female = [];\n                this.products.forEach(product => {\n                    if (product.category === 'Female') female.push(product)\n                })\n                return female;\n            }\n            return this.products\n        }\n    },\n    methods: {\n        ...mapActions({\n            getProducts: 'getProducts',\n        }),\n        selectedCategory(selected) {\n            this.selected = selected\n        },\n    },\n    mounted() {\n        // console.log(this.$route.name)\n        this.getProducts();\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.catalog {\n    .products {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        flex-wrap: wrap;\n\n        .catalog-item {\n            // background-color: $background-color;\n            background-color: orange;\n            flex-basis: 25%;\n            // box-shadow: $box-shadow;\n            margin: 8px;\n            padding: 8px;\n            img {\n                height: 200px;\n            }\n        }\n    }\n}\n\n\n\n\n\n@import url(\"https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,300;0,400;0,500;0,700;1,400&display=swap\");\n*,\n*::before,\n*::after {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"Roboto\", sans-serif;\n}\n.container {\n  position:absolute;\n  width:100%;\n  height:100vh;\n  background: #2c302d;\n}\n.card {\n  width: 60%;\n  height: 80%;\n  max-height: 300px;\n  padding: auto;\n  text-align: center;\n  background: #242625;\n  border-radius: 10px;\n  box-shadow: 25px 25px 50px #1b1c1b, -25px -25px 50px #2d302f;\n  &__container {\n    display: grid;\n    width:100%;\n    height:100%;\n    grid-template-columns: 1fr 1fr 1fr;\n    align-items: center;\n    justify-items: center;\n    color: white;\n  }\n  &__content {\n    width: 90%;\n    height: 95%;\n    background: #191a19;\n    margin: 10px auto;\n    border-radius: 5px;\n    padding: 20px;\n    cursor: pointer;\nbox-shadow:  16px 16px 44px #0a0a0a, \n             -16px -16px 44px #282a28;\n    transition: 0.3s all ease-in-out;\n    &:hover{\n      margin-top:-10px;\n    }\n  }\n  &__header {\n    text-transform: uppercase;\n    font-size: 20px;\n    margin: 40px auto;\n  }\n  &__button {\n    padding: 10px;\n    border-radius: 50px;\n    background: #1f75c4;\n    color: white;\n    font-weight: bold;\n    cursor: pointer;\n    border: none;\n    margin: 50px auto;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}