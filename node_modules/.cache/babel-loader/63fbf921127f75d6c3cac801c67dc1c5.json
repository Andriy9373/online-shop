{"ast":null,"code":"import CustomSelect from '@/components/Custom/CustomSelect';\nimport CustomRangeSlider from '@/components/Custom/CustomRangeSlider';\nimport { mapState, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCatalog',\n  components: {\n    CustomSelect,\n    CustomRangeSlider\n  },\n  props: {},\n\n  data() {\n    return {\n      options: [{\n        gender: 'Male'\n      }, {\n        gender: 'Female'\n      }, {\n        gender: 'All'\n      }],\n      selected: 'All',\n      labels: {\n        add_to_cart: 'Add to cart'\n      }\n    };\n  },\n\n  computed: { ...mapState({\n      products: 'products',\n      newProducts: 'newProducts',\n      cart: 'cart'\n    }),\n\n    sortedProducts() {\n      console.log(this.selected);\n\n      if (this.selected === 'Male') {\n        const male = [];\n        this.products.forEach(product => {\n          if (product.category === 'Male') male.push(product);\n        });\n        return male;\n      } else if (this.selected === 'Female') {\n        const female = [];\n        this.products.forEach(product => {\n          if (product.category === 'Female') female.push(product);\n        });\n        return female;\n      }\n\n      return this.products;\n    }\n\n  },\n  methods: { ...mapActions({\n      getProducts: 'getProducts',\n      getNewProducts: 'getNewProducts'\n    }),\n\n    selectedCategory(selected) {\n      this.selected = selected;\n    }\n\n  },\n\n  mounted() {\n    // console.log(this.$route.name)\n    this.getProducts();\n    this.getNewProducts();\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,OAAOA,YAAP,MAAyB,kCAAzB;AACA,OAAOC,iBAAP,MAA8B,uCAA9B;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAqC,MAArC;AAEA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE;IACRL,YADQ;IAERC;EAFQ,CAFD;EAMXK,KAAK,EAAE,EANI;;EAOXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,CACL;QAACC,MAAM,EAAE;MAAT,CADK,EAEL;QAACA,MAAM,EAAE;MAAT,CAFK,EAGL;QAACA,MAAM,EAAE;MAAT,CAHK,CADN;MAMHC,QAAQ,EAAE,KANP;MAOHC,MAAM,EAAE;QACJC,WAAW,EAAE;MADT;IAPL,CAAP;EAWH,CAnBU;;EAoBXC,QAAQ,EAAE,EACN,GAAGX,QAAQ,CAAC;MACRY,QAAQ,EAAE,UADF;MAERC,WAAW,EAAE,aAFL;MAGRC,IAAI,EAAE;IAHE,CAAD,CADL;;IAMNC,cAAc,GAAG;MACbC,OAAO,CAACC,GAAR,CAAY,KAAKT,QAAjB;;MACA,IAAI,KAAKA,QAAL,KAAkB,MAAtB,EAA8B;QAC1B,MAAMU,IAAG,GAAI,EAAb;QACA,KAAKN,QAAL,CAAcO,OAAd,CAAsBC,OAAM,IAAK;UAC7B,IAAIA,OAAO,CAACC,QAAR,KAAqB,MAAzB,EAAiCH,IAAI,CAACI,IAAL,CAAUF,OAAV;QACpC,CAFD;QAGA,OAAOF,IAAP;MACJ,CANA,MAOK,IAAI,KAAKV,QAAL,KAAkB,QAAtB,EAAgC;QACjC,MAAMe,MAAK,GAAI,EAAf;QACA,KAAKX,QAAL,CAAcO,OAAd,CAAsBC,OAAM,IAAK;UAC7B,IAAIA,OAAO,CAACC,QAAR,KAAqB,QAAzB,EAAmCE,MAAM,CAACD,IAAP,CAAYF,OAAZ;QACtC,CAFD;QAGA,OAAOG,MAAP;MACJ;;MACA,OAAO,KAAKX,QAAZ;IACJ;;EAvBM,CApBC;EA6CXY,OAAO,EAAE,EACL,GAAGvB,UAAU,CAAC;MACVwB,WAAW,EAAE,aADH;MAEVC,cAAc,EAAE;IAFN,CAAD,CADR;;IAKLC,gBAAgB,CAACnB,QAAD,EAAW;MACvB,KAAKA,QAAL,GAAgBA,QAAhB;IACH;;EAPI,CA7CE;;EAsDXoB,OAAO,GAAG;IACN;IACA,KAAKH,WAAL;IACA,KAAKC,cAAL;EACJ;;AA1DW,CAAf","names":["CustomSelect","CustomRangeSlider","mapState","mapActions","name","components","props","data","options","gender","selected","labels","add_to_cart","computed","products","newProducts","cart","sortedProducts","console","log","male","forEach","product","category","push","female","methods","getProducts","getNewProducts","selectedCategory","mounted"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CV/VUE-JS/online_shop/src/components/ShopCatalog.vue"],"sourcesContent":["<template>\n    <h1>Welcome!</h1>\n    <CustomSelect\n        :options=\"options\"\n        :selected=\"selected\"\n        @selected-category=\"selectedCategory\"\n    />\n    <CustomRangeSlider/>\n    <div class=\"card__container\">\n        <div\n            v-for=\"(item, index) in sortedProducts\"\n            :key=\"index\"\n            class=\"card\"\n        >\n            <div class=\"card__content\">\n                <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n                <h3 class=\"card__header\">{{ item.name }}</h3>\n                <p class=\"card__info\">{{ item.article }}</p>\n                <p>Price: {{ item.price }}â‚´</p>\n                <button\n                    class=\"card__button\"\n                    @click=\"$emit('addToCart', item)\"\n                >\n                    {{ labels.add_to_cart }}\n                </button>\n            </div>\n        </div>\n    </div>\n    {{ newProducts }}\n</template>\n\n<script>\nimport CustomSelect from '@/components/Custom/CustomSelect';\nimport CustomRangeSlider from '@/components/Custom/CustomRangeSlider';\nimport { mapState, mapActions } from 'vuex';\n\nexport default {\n    name: 'ShopCatalog',\n    components: {\n        CustomSelect,\n        CustomRangeSlider,\n    },\n    props: {},\n    data() {\n        return {\n            options: [\n                {gender: 'Male'},\n                {gender: 'Female'},\n                {gender: 'All'},\n            ],\n            selected: 'All',\n            labels: {\n                add_to_cart: 'Add to cart'\n            }\n        }\n    },\n    computed: {\n        ...mapState({\n            products: 'products',\n            newProducts: 'newProducts',\n            cart: 'cart',\n        }),\n        sortedProducts() {\n            console.log(this.selected)\n            if (this.selected === 'Male') {\n                const male = [];\n                this.products.forEach(product => {\n                    if (product.category === 'Male') male.push(product)\n                })\n                return male;\n            }\n            else if (this.selected === 'Female') {\n                const female = [];\n                this.products.forEach(product => {\n                    if (product.category === 'Female') female.push(product)\n                })\n                return female;\n            }\n            return this.products\n        }\n    },\n    methods: {\n        ...mapActions({\n            getProducts: 'getProducts',\n            getNewProducts: 'getNewProducts',\n        }),\n        selectedCategory(selected) {\n            this.selected = selected\n        },\n    },\n    mounted() {\n        // console.log(this.$route.name)\n        this.getProducts();\n        this.getNewProducts();\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.catalog {\n    .products {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        flex-wrap: wrap;\n\n        .catalog-item {\n            // background-color: $background-color;\n            background-color: orange;\n            flex-basis: 25%;\n            // box-shadow: $box-shadow;\n            margin: 8px;\n            padding: 8px;\n            img {\n                height: 200px;\n            }\n        }\n    }\n}\n\n\n\n\n\n@import url(\"https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,300;0,400;0,500;0,700;1,400&display=swap\");\n*,\n*::before,\n*::after {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"Roboto\", sans-serif;\n}\n\n.card {\n    border-radius: 10px;\n\n    &__container {\n        padding-top: 100px;\n        display: grid;\n        width:100%;\n        height:100%;\n        grid-template-columns: repeat(4, 1fr);\n        align-items: center;\n        justify-items: center;\n        color: white;\n    }\n\n    &__content {\n        color: #000000;\n        background: #e9e9e9;\n        margin: 30px auto;\n        border-radius: 5px;\n        padding: 30px;\n        box-shadow:  8px 8px 22px #e9d7d7, -8px -8px 22px #cddbcd;\n        transition: 0.3s all ease-in-out;\n\n        &:hover{\n            margin-top:-10px;\n        }\n\n        img {\n            height: 200px;\n            width: 200px;\n        }\n    }\n\n    &__header {\n        text-transform: uppercase;\n        font-size: 20px;\n        margin: 40px auto;\n    }\n\n    &__button {\n        padding: 10px;\n        border-radius: 50px;\n        background: #2196F3;\n        color: white;\n        font-weight: bold;\n        cursor: pointer;\n        border: none;\n        margin: 20px auto;\n        transition: .3s ease;\n\n        &:hover {\n            opacity: .8;\n        }\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}