{"ast":null,"code":"import CustomSelect from '@/components/CustomSelect';\nimport { mapState, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCatalog',\n  components: {\n    CustomSelect\n  },\n  props: {},\n\n  data() {\n    return {\n      minPrice: 0,\n      maxPrice: 1000,\n      options: [{\n        gender: 'Male'\n      }, {\n        gender: 'Female'\n      }, {\n        gender: 'All'\n      }],\n      selected: 'All',\n      labels: {\n        add_to_cart: 'Add to cart'\n      }\n    };\n  },\n\n  computed: { ...mapState({\n      products: 'products',\n      cart: 'cart'\n    }),\n\n    sortedProducts() {\n      console.log(this.selected);\n\n      if (this.selected === 'Male') {\n        const male = [];\n        this.products.forEach(product => {\n          if (product.category === 'Male') male.push(product);\n        });\n        return male;\n      } else if (this.selected === 'Female') {\n        const female = [];\n        this.products.forEach(product => {\n          if (product.category === 'Female') female.push(product);\n        });\n        return female;\n      }\n\n      return this.products;\n    }\n\n  },\n  methods: { ...mapActions({\n      getProducts: 'getProducts'\n    }),\n\n    selectedCategory(selected) {\n      this.selected = selected;\n    }\n\n  },\n\n  mounted() {\n    // console.log(this.$route.name)\n    this.getProducts();\n  }\n\n};","map":{"version":3,"mappings":"AA0CA,OAAOA,YAAP,MAAyB,2BAAzB;AACA,SAASC,QAAT,EAAmBC,UAAnB,QAAqC,MAArC;AAEA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAU,EAAE;IACRJ;EADQ,CAFD;EAKXK,KAAK,EAAE,EALI;;EAMXC,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,CADP;MAEHC,QAAQ,EAAE,IAFP;MAGHC,OAAO,EAAE,CACL;QAACC,MAAM,EAAE;MAAT,CADK,EAEL;QAACA,MAAM,EAAE;MAAT,CAFK,EAGL;QAACA,MAAM,EAAE;MAAT,CAHK,CAHN;MAQHC,QAAQ,EAAE,KARP;MASHC,MAAM,EAAE;QACJC,WAAW,EAAE;MADT;IATL,CAAP;EAaH,CApBU;;EAqBXC,QAAQ,EAAE,EACN,GAAGb,QAAQ,CAAC;MACRc,QAAQ,EAAE,UADF;MAERC,IAAI,EAAE;IAFE,CAAD,CADL;;IAKNC,cAAc,GAAG;MACbC,OAAO,CAACC,GAAR,CAAY,KAAKR,QAAjB;;MACA,IAAI,KAAKA,QAAL,KAAkB,MAAtB,EAA8B;QAC1B,MAAMS,IAAG,GAAI,EAAb;QACA,KAAKL,QAAL,CAAcM,OAAd,CAAsBC,OAAM,IAAK;UAC7B,IAAIA,OAAO,CAACC,QAAR,KAAqB,MAAzB,EAAiCH,IAAI,CAACI,IAAL,CAAUF,OAAV;QACpC,CAFD;QAGA,OAAOF,IAAP;MACJ,CANA,MAOK,IAAI,KAAKT,QAAL,KAAkB,QAAtB,EAAgC;QACjC,MAAMc,MAAK,GAAI,EAAf;QACA,KAAKV,QAAL,CAAcM,OAAd,CAAsBC,OAAM,IAAK;UAC7B,IAAIA,OAAO,CAACC,QAAR,KAAqB,QAAzB,EAAmCE,MAAM,CAACD,IAAP,CAAYF,OAAZ;QACtC,CAFD;QAGA,OAAOG,MAAP;MACJ;;MACA,OAAO,KAAKV,QAAZ;IACJ;;EAtBM,CArBC;EA6CXW,OAAO,EAAE,EACL,GAAGxB,UAAU,CAAC;MACVyB,WAAW,EAAE;IADH,CAAD,CADR;;IAILC,gBAAgB,CAACjB,QAAD,EAAW;MACvB,KAAKA,QAAL,GAAgBA,QAAhB;IACH;;EANI,CA7CE;;EAqDXkB,OAAO,GAAG;IACN;IACA,KAAKF,WAAL;EACJ;;AAxDW,CAAf","names":["CustomSelect","mapState","mapActions","name","components","props","data","minPrice","maxPrice","options","gender","selected","labels","add_to_cart","computed","products","cart","sortedProducts","console","log","male","forEach","product","category","push","female","methods","getProducts","selectedCategory","mounted"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CV/VUE-JS/online_shop/src/components/ShopCatalog.vue"],"sourcesContent":["<template>\n    <h1>Catalog!</h1>\n    <CustomSelect\n        :options=\"options\"\n        :selected=\"selected\"\n        @selected-category=\"selectedCategory\"\n    />\n    <div>\n        <input \n            type=\"range\"\n            min=\"0\"\n            max=\"1000\"\n            v-model=\"minPrice\"\n        />\n        <input \n            type=\"range\"\n            min=\"0\"\n            max=\"1000\"\n            v-model=\"maxPrice\"\n        />\n        {{ maxPrice }}|||{{ minPrice }}\n    </div>\n    <div class=\"catalog\">\n        <div class=\"products\">\n            <div\n                class=\"catalog-item\"\n                v-for=\"(item, index) in sortedProducts\"\n                :key=index\n            >\n                <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n                <h2>{{ item.name }}</h2>\n                <p>{{ item.article }}</p>\n                <p>{{ item.price }}</p>\n                <button @click=\"$emit('addToCart', item)\">\n                    {{ labels.add_to_cart }}\n                </button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport CustomSelect from '@/components/CustomSelect';\nimport { mapState, mapActions } from 'vuex';\n\nexport default {\n    name: 'ShopCatalog',\n    components: {\n        CustomSelect\n    },\n    props: {},\n    data() {\n        return {\n            minPrice: 0,\n            maxPrice: 1000,\n            options: [\n                {gender: 'Male'},\n                {gender: 'Female'},\n                {gender: 'All'},\n            ],\n            selected: 'All',\n            labels: {\n                add_to_cart: 'Add to cart'\n            }\n        }\n    },\n    computed: {\n        ...mapState({\n            products: 'products',\n            cart: 'cart',\n        }),\n        sortedProducts() {\n            console.log(this.selected)\n            if (this.selected === 'Male') {\n                const male = [];\n                this.products.forEach(product => {\n                    if (product.category === 'Male') male.push(product)\n                })\n                return male;\n            }\n            else if (this.selected === 'Female') {\n                const female = [];\n                this.products.forEach(product => {\n                    if (product.category === 'Female') female.push(product)\n                })\n                return female;\n            }\n            return this.products\n        }\n    },\n    methods: {\n        ...mapActions({\n            getProducts: 'getProducts',\n        }),\n        selectedCategory(selected) {\n            this.selected = selected\n        },\n    },\n    mounted() {\n        // console.log(this.$route.name)\n        this.getProducts();\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.catalog {\n    .products {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        flex-wrap: wrap;\n\n        .catalog-item {\n            // background-color: $background-color;\n            background-color: orange;\n            flex-basis: 25%;\n            // box-shadow: $box-shadow;\n            margin: 8px;\n            padding: 8px;\n            img {\n                height: 200px;\n            }\n        }\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}