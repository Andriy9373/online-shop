{"ast":null,"code":"//import ShopCartItem from '@/components/ShopCartItem'\nimport { mapState, mapActions } from 'vuex';\nexport default {\n  name: 'ShopCart',\n  components: {//ShopCartItem,\n  },\n\n  data() {\n    return {\n      labels: {\n        delete: 'Delete'\n      }\n    };\n  },\n\n  props: {},\n  computed: { ...mapState({\n      cart: 'cart'\n    })\n  },\n  methods: { ...mapActions({\n      deleteProductFromCart: 'deleteProductFromCart'\n    }),\n\n    deleteProduct(index) {\n      this.deleteProductFromCart(index);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsBA;AAEA,SAASA,QAAT,EAAmBC,UAAnB,QAAqC,MAArC;AAEA,eAAe;EACXC,IAAI,EAAE,UADK;EAEXC,UAAU,EAAE,CACR;EADQ,CAFD;;EAKXC,IAAI,GAAG;IACH,OAAO;MACHC,MAAM,EAAE;QACJC,MAAM,EAAE;MADJ;IADL,CAAP;EAKH,CAXU;;EAYXC,KAAK,EAAE,EAZI;EAaXC,QAAQ,EAAE,EACN,GAAGR,QAAQ,CAAC;MACRS,IAAI,EAAE;IADE,CAAD;EADL,CAbC;EAkBXC,OAAO,EAAE,EACL,GAAGT,UAAU,CAAC;MACXU,qBAAqB,EAAE;IADZ,CAAD,CADR;;IAILC,aAAa,CAACC,KAAD,EAAQ;MACjB,KAAKF,qBAAL,CAA2BE,KAA3B;IACJ;;EANK;AAlBE,CAAf","names":["mapState","mapActions","name","components","data","labels","delete","props","computed","cart","methods","deleteProductFromCart","deleteProduct","index"],"sourceRoot":"","sources":["/home/andrii/Hirniak_Andrii/Projects for CD/VUE-JS/online_shop/src/components/ShopCart.vue"],"sourcesContent":["<template>\n    <div>\n        <h1>Cart</h1>\n        <router-link :to=\"{name: 'Catalog'}\" class=\"cart__link_to_catalog\">\n            Back to catalog\n        </router-link>\n        <!-- <ShopCartItem/> -->\n        <div class=\"cart\" v-for=\"(item, index) in cart\" :key=\"index\">\n            <img :src=\"require('@/assets/images/' + item.image)\" alt=\"product\"/>\n            <p>{{ item.name }}</p>\n            <p>{{ item.article }}</p>\n            <p>{{ item.price }}</p>\n            <p>QUANTITY: {{ item.quantity }}</p>\n            <button type=\"button\" @click=\"deleteProduct(index)\">{{ labels.delete }}</button>\n        </div>\n        <div v-if